  "compilerOptions": {
    "target": "es2018",
    "lib": ["webworker", "es2018"],
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
